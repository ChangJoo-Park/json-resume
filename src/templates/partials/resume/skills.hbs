{{#> section}}
  {{#each data as |skill|}}
    <div class="detail-item">
      <header class="header">
        <h3>{{skill.name}}</h3>
      </header>
      <p class="subheader-1">{{skill.level}}</p>
      <div class="further-info">
        <ul class="inline-list">
          {{#each skill.keywords as |keyword|}}
            <li>{{keyword}}</li>
          {{/each}}
        </ul>
      </div>
    </div>
  {{/each}}
{{/section}}
